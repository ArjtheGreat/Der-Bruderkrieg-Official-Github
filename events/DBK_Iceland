 add_namespace = ICE_alliance_with_norway

 country_event = {
	id = ICE_alliance_with_norway.1
	title = ICE_alliance_with_norway.1.t
	desc = ICE_alliance_with_norway.1.d
	picture = GFX_holyalliance

	fire_only_once = yes   #this means this event can only fire one time
	is_triggered_only = yes #this means this event can't happen randomly, has to be triggered somewhere else. 

	option = { # This option will give the war goal
		name = ICE_alliance_with_norway.1.a
		ai_chance = { factor = 50000
		} #this factor means the AI will almost always pick this one
		set_country_flag = aliance_nor_yes

		
	}

	option = { # Last chance to backout! Click this option and literally nothing happens
		name = ICE_alliance_with_norway.1.b
	}
}